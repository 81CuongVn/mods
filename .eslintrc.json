{
  "$schema": "https://json.schemastore.org/eslintrc",
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "standard"
  ],
  "globals": {
    "alert": "readonly",
    "Atomics": "readonly",
    "Audio": "readonly",
    "cache": "readonly",
    "member": "readonly",
    "process": "readonly",
    "server": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "rules": {
    "linebreak-style": 2,
    "no-case-declarations": 0,
    "no-eval": 0,
    "no-new": 0,
    "no-prototype-builtins": 0,
    "no-useless-constructor": 0,
    "object-shorthand": [
      2,
      "always"
    ],
    "prefer-promise-reject-errors": 0
  }
}
